cd C:\Users\Korenyk.A\Downloads\pgsql\bin
.\pg_ctl.exe start -D "C:\Users\Korenyk.A\Downloads\pgsql\data"
netstat -ano | findstr 5432
.\psql.exe -U machine_tools
.\psql -U postgres -c "SELECT pg_terminate_backend(pid) FROM pg_stat_activity WHERE datname = 'machine_tools';"
.\dropdb.exe -U postgres machine_tools

alembic stamp head
alembic upgrade head
alembic revision --autogenerate -m "add new_column"

бекап
.\pg_dump.exe -U postgres machine_tools > "C:\Users\Korenyk.A\Documents\Проекты\machine_tools\database_backups\machine_tools_backup_1.sql"

восстановление
.\psql.exe -U postgres -d machine_tools < "C:\Users\Korenyk.A\Documents\Проекты\machine_tools\machine_tools_backup.sql"

в CSV
psql -U postgres -d machine_tools -c "\copy (SELECT * FROM machine_tools) TO 'machine_tools.csv' WITH CSV HEADER"
psql -U postgres -d machine_tools -c "\copy (SELECT * FROM technical_requirements) TO 'technical_requirements.csv' WITH CSV HEADER"

.\psql -U postgres -d machine_tools -c "COPY machine_tools TO 'C:\Users\Korenyk.A\Documents\Проекты\machine_tools\database_backups\machine_tools.csv' WITH CSV HEADER"
.\psql -U postgres -d machine_tools -c "\copy (SELECT * FROM technical_requirements) TO 'C:\Users\Korenyk.A\Documents\Проекты\machine_tools\database_backups\technical_requirements.csv' WITH CSV HEADER"

CREATE DATABASE machine_tools
     WITH
     OWNER = postgres
     ENCODING = 'UTF8'
     LC_COLLATE = 'Russian_Russia.1251'
     LC_CTYPE = 'Russian_Russia.1251'
     TEMPLATE = template0;